{
  "name": "rda-annotator",
  "description": "RDA Annotator - a browser extension to create and view annotations for research data management resources",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
